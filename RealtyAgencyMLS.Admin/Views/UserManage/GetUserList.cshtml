
@{
    ViewData["Title"] = "GetUserList";
}
<div class="row">
    <div class="col-md-12">
        <div class="card custom-card overflow">
            <div class="col-md-12">
                
                <h2 class="main-head">Manage Users</h2>
                <div class="row">
                    <div class="col-md-6">
                        <button type="button" id="btnAddNewUser" class="btn btn-sm btn-info mr-2">Add New User</button>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group row">
                            <label class="col-6 pt-1 text-right">Role:</label>
                            <div class="col-6">
                                <select id="ddlUserType" class="form-control">
                                    <option value="Agent">Agent</option>
                                    <option value="Admin">Admin</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
                <hr />
                <table id="tblMyUsers" class="table table-striped table-bordered dt-responsive nowrap">
                    <thead>
                        <tr>
                            <th>First Name</th>
                            <th>Last Name</th>
                            <th>Email</th>
                            <th>Phone Number</th>
                            <th>Address</th>
                            <th>City</th>
                            <th>Role</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<partial name="_CreateUser" model="@(new UserViewModel())" />
<div id="editPartial"></div>
<script src="~/js/CustomJS/ManageUser.js"></script>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
